* {
    box-sizing: border-box;
}
body
{
    width: 100%;
    margin: 0px;
    padding: 0px;
    overflow-x: hidden;
    overflow-y: auto;
    font-family: Arial, Helvetica, sans-serif;
    background-color: #F0F6FF;
}
.menu
{
    width: 100%;
    height: 70px;
    background-color:#0052cd;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 5px;
    font-weight: bold;
}
.menu img
{
    height: 50px;
}
.menulink 
{
    list-style: none;
    display: flex;
    align-items: center;
    margin: 0;
    padding: 0;
}

.menulink li 
{
    position: relative;
}

.menulink li a 
{
    color: #F0F6FF;
    text-decoration: none;
    font-size: 23px;
    margin: 0 20px;
}
.menulink li a:hover
{
    color: #FACC15;
    text-shadow: 0 0 20px #ddff00;
}

.hesabim-menu 
{
    list-style: none;
    position: absolute;
    top: 105%;
    right: -10px;
    background-color: #0052cd;
    padding: 10px 0;
    border-radius: 10px;
    box-shadow: 0 5px 15px black;
    display: none;
    z-index: 999;
}

.hesabim-menu li a 
{
    display: block;
    padding: 10px 20px;
    font-size: 18px;
}

.hesabim-menu li a:hover 
{
    color: #FACC15;
    text-shadow: 0 0 20px #ddff00;
}
.hesabim:hover .hesabim-menu 
{
    display: block;
}
.tablo 
{
    width: 100%;
    text-align: center;
    margin:40px auto; 
}
.tablo h1
{
    text-align: center;
    font-size: 50px;
    font-weight: bolder;
}
.kartlar {
    display: flex;
    justify-content: center;
    align-items: center;
    margin:auto;
}

.sol, .sag
{
    width:31%;
	height:400px;
	background-color:#0052cd; 
	border: 3px solid #0059ff;
	margin:10px; 
    border-radius: 20px;
    position: relative;
    top: 30px;
    transition: 1s;
    transform-style: preserve-3d;
    box-shadow: 0 0 15px black;    
}
.orta
{
    width:31%;
	height:400px;
	background-color:#0052cd; 
	border: 3px solid #0059ff;
	margin:10px; 
    border-radius: 20px;
    transition: 1s;
    transform-style: preserve-3d;
    box-shadow: 0 0 15px black;
}
.sol:hover,.orta:hover,.sag:hover
{
    background-color:#FACC15; 
    border: 3px solid #edc00d;
    transition: 1s;
    transform: translateY(-10px) rotateX(5deg) rotateY(5deg) scale(1.02);
}
